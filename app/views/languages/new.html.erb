
<div class="formContainer">
<h1> Create a new Language-Level </h1>

<%= form_for @language do |f| %>

  <p>
    <%= f.label :level %><br>
    <%= f.text_field :level %>
  </p>


  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>


  <%= f.submit %>

<% end %>

</div>
